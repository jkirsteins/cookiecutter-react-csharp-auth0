{
  "CheckConfigKeys": true,
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },

  "Auth0": {
    "Domain": "{{cookiecutter.auth0.domain}}",
    "Audience": "{{cookiecutter.auth0.audience}}",
    "ClientSecret": "{{cookiecutter.auth0.client_secret}}"
  },
  "Frontend": {
    "Url": "{{cookiecutter.frontend.url}}",
    "CorsOrigins": "{{cookiecutter.frontend.cors_origins}}",
    "CookieDomain": "{{cookiecutter.frontend.cookie_domain}}"
  },

  "AllowedHosts": "*"
}
